<table class="table">
  <thead>
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
    </tr>
  </thead>
  <tbody>
    <% for (let user of users) { %>
      <tr>
        <td>
          <%= user.firstName %>
        </td>
        <td>
          <%= user.lastName %>
        </td>
      </tr>
      <%}%>

  </tbody>
</table>