<table class="table">
  <thead>
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Address1</th>
      <th scope="col">Address1</th>
      <th scope="col">City</th>
      <th scope="col">State</th>
      <th scope="col">Zip</th>
      <th scope="col">Country</th>
      <th scope="col">Date</th>
    </tr>
  </thead>
  <tbody>
    <% for (let user of users) { %>
      <tr>
        <td>
          <%= user.firstName %>
        </td>
        <td>
          <%= user.lastName %>
        </td>
        <td>
          <%= user.addressOne %>
        </td>
        <td>
          <%= user.addressTwo %>
        </td>
        <td>
          <%= user.city %>
        </td>
        <td>
          <%= user.state %>
        </td>
        <td>
          <%= user.zip %>
        </td>
        <td>
          <%= user.country %>
        </td>
        <td>
          <%= user.createAt.toLocaleDateString()+"-"+ user.createAt.toLocaleTimeString()%>
        </td>
      </tr>
      <%}%>

  </tbody>
</table>